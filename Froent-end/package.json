{
  "name": "nextunite-tech",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.mts",
    "start:app": "vite --config vite.config.mts",
    "start:mock": "json-server --watch db.json --port 4000",
    "start": "concurrently \"npm run start:app\" \"npm run start:mock\"",
    "build": "tsc && vite build --config vite.config.mts",
    "build:prod": "NODE_ENV=production npm run build",
    "preview": "vite preview --config vite.config.mts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t nextunitech-frontend .",
    "docker:run": "docker run -p 3000:80 nextunitech-frontend"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.18.0",
    "three": "^0.158.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/jest": "^29.5.7",
    "@types/json-schema": "^7.0.15",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/stats.js": "^0.17.4",
    "@types/three": "^0.158.2",
    "@types/webxr": "^0.5.24",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^4.0.7",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
